const elem = document.getElementById("restartModal");
(new bootstrap.Modal(elem)).hide();

const progressModal = document.getElementById("progress-modal");
const progressBar = progressModal.getElementsByClassName('.progress-bar');

(new bootstrap.Modal(progressModal)).show();
$(progressBar).addClass('animate');

setTimeout(function() {
  $(progressBar).removeClass('animate');
  (new bootstrap.Modal(progressModal)).hide();
}, 10000);
