<table border=1>
  <tr>
    <th>id</th>
    <th>type</th>
    <th>body</th>
    <th>hostname</th>
  </tr>
  <% @events.each do |event| %>
    <tr>
      <td><%= event.id %></td>
      <td><%= event.event_type %></td>
      <td><%= event.body %></td>
      <td><%= event.hostname %></td>
    </tr>
  <% end %>
</table>
